buyItem.content{
    min-width:1000px !important;
}
*{
    outline:none;
}
.item{
    width:100%;
    padding:0;
    box-sizing:border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
}
.bread{
    font-family:'Open sans','Montserrat',Arial, Helvetica, sans-serif;
    font-size:13px;
    line-height:10px;
    display:block;
    height:15px;
    overflow: hidden;
}
.bread>div{
    float:left;
}
.bread .itm_sp{
    color:#999;
}
.bread .itm_sp>b{
    color:#666;
}
.bread .itm_a>a{
    text-decoration: none;
    color:#44f;
}
.bread .itm_a , .bread .itm_sp{
    width:auto;
    position: relative;
    padding:0 5px;
    max-width:100px;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    height:15px;
    vertical-align: middle;
}

.bread>div:hover{
    width:fit-content;
    max-width:fit-content !important;
    white-space:nowrap;
    text-overflow: inherit;
}
#itm_product{
    width:100%;
    font-family:'Open sans','Montserrat',Arial, sans-serif;
    box-sizing:border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    padding:20px 10px;
    /* background:#ccc; */
    max-width:1200px;
    margin:0 auto;
}
#itm_product>.item{
    padding:0;
    margin-bottom:0;
    display:flex;
    height:100%;
    flex-direction:row;
}
#itm_product #itm_lft{
    flex-basis:500px;
}
#itm_product #itm_rt{
    flex-basis: calc(100% - 500px);
}


#itm_img{
    width:480px;
    height:550px;
    padding:20px 10px;
    /* margin:0 10px; */
    border:solid 1px #fff;
    border-radius: 10px;
    box-shadow:0 0 2px 0 rgba(0,0,0,0.2);
    display: flex;
    flex-direction:column;
    position:sticky;
    top:20px;
}

#itm_pre,#itm_pre>div{
    width:50px;
    height:450px;
    display:flex;
    position: relative;
    top:25px;
    flex-direction:column;
    align-content: center;
    justify-content: center;
    transition: height 0.5s cubic-bezier(0.47, 0, 0.745, 0.715);
}

#itm_pre>div>.itm_pre_div{
    position: relative;
    width:45px;
    height:45px;
    margin:10px 2.5px;
    box-shadow:0 0 2px 1px rgba(0,10,100,0.25);
    border-radius:3px; 
    overflow:hidden;
    overflow-x:hidden;
    overflow-y:hidden;
    cursor:pointer;
}
#itm_pre>div>.itm_pre_div:hover,#itm_pre>div>.itm_pre_div[open]{
    box-shadow:0 0 2px 1px rgb(255, 0, 0);
}
#itm_pre:not([open])>div>.itm_pre_div[hide]{
    display:none;
}
#itm_name>h2{
    font-size:28px !important;
}
#itm_pre[open]>div>.itm_pre_div[hide]{
    display:block;
}
#itm_pre[open] .itm_pre_div2{
    display:none;
}

#itm_pre>div>.itm_pre_div>img{
    width:100%;
    position: relative;
    top:50%;
    left:50%;
    transform:translateX(-50%) translateY(-50%);
}

.itm_pre_div#more>div{
    width:100%;
    height:100%;
    position:absolute;
    top:0;
    left:0;
    background:rgba(0,0,0,0.75);
    text-align:center;
    color:#fff;

}

.itm_pre_div#more>div::after{
    width:100%;
    content:attr(more);
    font-size:15px;
    line-height:15px;
}

