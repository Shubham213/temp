#itm_pre_scr{
    position:absolute;
    padding:0;
    top:20px;
    left:10px;
    width:50px;
    height:500px;
    flex-basis:40px;
    box-sizing: border-box;
    /* background:#444; */
    display:flex;
    flex-basis:column;
}
#itm_pre_scr>.itm_pre_div,#itm_pre_scr>.itm_nxt_div{
    width:30px;
    height:20px;
    border-radius: 5px;
    box-shadow:0 0 2px 0px rgba(0,0,0,0.1);
    padding:0 10px;
    background:#fff;
    position:absolute;
    top:0;
    left:0;
    cursor:pointer;
    overflow-x:hidden;
    overflow-y:hidden;
}
#itm_pre_scr>.itm_nxt_div{
    top:480px;
    left:0;
}
#itm_pre_scr[track="1"]>.pre{
    /* display:none; */
    opacity:0.2;
    pointer-events: none;
}

#itm_pre_scr[track="2"]>.nxt{
    /* display:none; */
    opacity:0.2;
    pointer-events: none;
}
.nxt::before,.pre::before{
    content:"";
    width:20px;
    height:20px;
    position: absolute;
    top:50%;
    left:50%;
    background:#444;
    transform:translateX(-50%) translateY(-75%) rotateZ(45deg);
    transform-origin: center;
}
#itm_pre>div{
    top:0;
}
.nxt::after,.pre::after{
    content:"";
    width:20px;
    height:20px;
    position: absolute;
    top:50%;
    left:50%;
    background:#fff;
    transform:translateX(-50%) translateY(-100%) rotateZ(45deg);
    transform-origin: center;
}

.pre::before{ transform:translateX(-50%) translateY(-25%) rotateZ(45deg); }
.pre::after{ transform:translateX(-50%) translateY(0%) rotateZ(45deg); }

#itm_pre_scr:not([open]){
    display: none;
}

#itm_pre[track="1"]>div:not([track="1"]){
    /* display:none; */
    height:0;
    overflow:hidden;
}
#itm_pre[track="2"]>div:not([track="2"]){
    /* display:none; */
    height:0;
    overflow:hidden;
}

#itm_pre{
    overflow-x: hidden;
    overflow-y:hidden;
}

#itm_preview{
    position: absolute;
    top:0;
    left:10px;
    width:calc(100% - 10px);
    height:100%;
    cursor:pointer;
}
#itm_preview>div{
    text-shadow:-1px 0 8px rgb(0,0,0); width:100%; text-align:center; position:absolute; top:-10px; color:#fff; opacity:0;
}
#itm_preview:hover>div{
    display:block;
    animation:2s anime linear forwards;
}
#itm_preview:hover>div>span{
    background:rgba(153, 153, 153, 0.4); padding:2.5px 10px; border-radius:15px;
}
@keyframes anime{
    0%,100%{
        opacity:0;
    }
    20%,80%{
        opacity:1;
    }
}

